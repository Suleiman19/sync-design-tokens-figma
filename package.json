{
  "version": "1.0.0",
  "description": "Automatically create and update Figma Styles from a design tokens url.",
  "author": "Suleiman Ali Shakir",
  "license": "MIT",
  "dependencies": {
    "@create-figma-plugin/ui": "^2.1.0",
    "@create-figma-plugin/utilities": "^2.1.0",
    "preact": "^10.9.0"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.1.0",
    "@create-figma-plugin/tsconfig": "^2.1.0",
    "@figma/plugin-typings": "1.47.0",
    "typescript": "^4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "sync-design-tokens",
    "name": "sync-design-tokens",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}